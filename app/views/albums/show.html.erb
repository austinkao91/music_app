<h1><%= @album.name %>'s Tracks</h1>
<h2>Band name: <%= @album.band.name %></h2>

<ul>
<% @album.tracks.each do |track| %>
  <li>
    <%= link_to track.name, track_url(track) %>
  </li>
<% end %>
</ul>
<br>
<%= link_to "New Track", new_album_track_url(@album) %>
<br>
<%= link_to "Edit Album", edit_album_url(@album) %>
<br>
<%= link_to "Back to band", band_url(@album.band) %>
<br>
<%= link_to "Delete Album", album_url(@album), method: :delete %>
